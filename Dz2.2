#include <iostream>
#include <string>


using namespace std;

int main()
{
    string s;
    cout << "Введите строку: ";
    cin >> s;
    
    cout << "Введите разделитель: ";
    char a;
    cin >> a;
    
    while (s.find(a)!= -1)
    {
        cout << s.substr(0, s.find(a)) << endl;
        s.erase(0, s.find(a)+1);
        
    }
    cout << s;
    

    return 0;
}
