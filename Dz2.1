#include <iostream>
#include <string>

using namespace std;

int main()
{
    int m;
    cout << "Введите m:";
    cin >> m;
    int a[m];
    cout << "Ввдите вектор:"<< endl;
    for (int i=0; i<m; i++) cin >> a[i];
    
    
    int n;
    cout << "Введите n:";
    cin >> n ;
    int b[n][m];
    cout << "Ввдите матрицу:" << endl;
    for (int i=0; i<n; i++) 
    {
        for (int j=0; j<m; j++)
        {
            cin >> b[i][j];
        }
    }
    
    
    cout << "Ответ:"<< endl;
    for (int i=0; i<n; i++)
    {
        int s=0;
        for (int j=0; j<m; j++)
        {
        s+= b[i][j]*a[j];
        }
        
        cout <<s << endl;
    }
    
    return 0;
    
}
